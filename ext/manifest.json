{
    "manifest_version": 3,
    "name": "Inwen",
    "version": "1.0",
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Inwen Settings"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}